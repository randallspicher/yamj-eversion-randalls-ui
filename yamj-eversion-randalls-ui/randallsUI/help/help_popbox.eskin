	<segment>
		<name>helpbar</name>
		<control>[:@eskin:]help</control>
		<hyperscroll>false</hyperscroll>
		<startx>120</startx>
		<starty>70</starty>
		<scroll>1</scroll>
		<tile>
			<cols>4</cols>
			<rows>1</rows>
			<width>200</width>
			<height>55</height>
			<text>
				<name>helper</name>
				<display>[:title:]</display>
				<posx>0</posx>
				<posy>0</posy>
				<width>200</width>
				<height>55</height>
				<size>30</size>
				<align>center</align>
				<color>808080</color>
				<hlcolor>FFFFFF</hlcolor>
			</text>	
		</tile>
	</segment>
