<?xml version="1.0" encoding="UTF-8"?>

<eskin>

<control>
<fullscreen>true</fullscreen>
</control>

	<!-- Background -->

<background>
<file>[:@media:]common/tv.banner.(3).0.background.png</file>
</background>

	<!-- Fanart -->

<image>
<name>fanart</name>
<file>[:@jukebox:][:fanart:]</file>
<posx>13</posx>
<posy>13</posy>
<width>1254</width>
<height>658</height>
</image>

	<!-- Details Bar 1 - Bottom -->

<image>
<name>details.bar1</name>
<file>[:@media:]common/tv.banner.(3).2.outline.half.outline1.png</file>
<posx>13</posx>
<posy>331</posy>
<width>1254</width>
<height>340</height>
</image>

	<!-- Video Image - Unknown -->

<image>
<name>video.image.unknown</name>
<file>[:@media:]common/tv.banner.(3).2.outline.half.unknown.png</file>
<posx>65</posx>
<posy>350</posy>
<width>300</width>
<height>170</height>
</image>

	<!-- Video Image -->

<image>
<name>video.image</name>
<swap>true</swap>
<segname>eplist</segname>
<hyper>3</hyper>
<file>[:@jukebox:][:videoimage:]</file>
<posx>65</posx>
<posy>350</posy>
<width>300</width>
<height>170</height>
</image>

	<!-- Remote -->

<remote>
<button>PLAY</button>
<action>PLAYFROMHERE</action>	
</remote>		

<remote>
<button>PAUSE</button>
<action>PLAYRANDOM</action>	
</remote>		

<remote>
<button>SELECT</button>
<action>PLAYSINGLE</action>	
</remote>	

<remote>
<button>FASTFORWARD</button>
<action>PLAYLAST</action>	
</remote>		

<remote>
<button>BACK</button>
<action>BACK</action>
<data>1</data>
</remote>

<remote>
<button>PAGEUP</button>
<action>EXTRA</action>	
<file>tv.banner.(3).1.outline.full</file>
</remote>

<remote>
<button>PAGEDOWN</button>
<action>EXTRA</action>	
<file>tv.banner.(3).3.episodes</file>
</remote>

<remote>
<button>RED</button>
<action>HOME</action>
</remote>

<remote>
<button>REPEAT</button>
<action>RESET</action>
<title>[:%reset:]</title>
</remote>

<remote>
<button>ALL</button>
<action>BLOCK</action>
</remote>

	<!-- Total Tiles, ie. 1/18 -->

<text>
<name>episode.total.tiles</name>
<segname>eplist</segname>
<hyper>4</hyper>
<display>[:episode:]/[:@totaltiles:]</display>
<posx>65</posx>
<posy>523</posy>
<width>370</width>
<height>25</height>
<size>16</size>  
<bold>true</bold>
<align>left</align>
<color>tvb3.2.aired.label.color</color>
</text>	

	<!-- Episode Aired -->

<text>
<name>episode.aired</name>
<segname>eplist</segname>
<hyper>4</hyper>
<display>[:aired,blank:]</display>
<posx>155</posx>
<posy>523</posy>
