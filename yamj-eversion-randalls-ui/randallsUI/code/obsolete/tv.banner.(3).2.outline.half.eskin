<segment>

	<!-- Episodes List - Start Location -->

<name>eplist</name>
<hyperscroll>false</hyperscroll>
<datasource>episodes</datasource>
<startx>850</startx>
<starty>350</starty>
<endx>1180</endx>
<endy>650</endy>
<scroll>2</scroll>

	<!-- Episodes List - Cursor Size -->

<cursor>
<file>[:@media:]common/cursor/cursor_refl[:#cursor.refl:].950.100.5.3.[:#cursor.color:].[:#cursor.shiny:].swf</file>
<above>true</above>
<animate>true</animate>
<posx>-5</posx>
<posy>0</posy>
<width>380</width>
<height>23</height>
<scroll>2</scroll>
</cursor>

<tile>

	<!-- Columns and Rows -->

<cols>1</cols>
<rows>14</rows>
<width>620</width>
<height>22</height>

	<!-- Episode Title -->

<text>
<name>episode.title</name>
<display>[:episode:]. [:title,trun45:]</display>
<posx>0</posx>
<posy>0</posy>
<width>370</width>
<height>25</height>
<size>18</size>  
<bold>bold</bold>
<align>center</align>
